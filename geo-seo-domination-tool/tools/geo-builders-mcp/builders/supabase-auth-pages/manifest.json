{
  "id": "supabase-auth-pages",
  "title": "Supabase Auth Pages + Middleware",
  "summary": "Login/logout UI (magic link + OAuth), protected /dashboard, server-side Supabase helper, and account page.",
  "engine": "eta",
  "variables": [
    {
      "name": "LOGIN_PATH",
      "required": false,
      "example": "login"
    },
    {
      "name": "DASHBOARD_PATH",
      "required": false,
      "example": "dashboard"
    },
    {
      "name": "OAUTH_PROVIDERS",
      "required": false,
      "example": "google,github"
    }
  ],
  "files": [
    {
      "from": "templates/app/login/page.tsx.eta",
      "to": "web-app/app/<%= LOGIN_PATH || 'login' %>/page.tsx",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/app/account/page.tsx.eta",
      "to": "web-app/app/account/page.tsx",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/app/dashboard/page.tsx.eta",
      "to": "web-app/app/<%= DASHBOARD_PATH || 'dashboard' %>/page.tsx",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/lib/supabaseServer.ts.eta",
      "to": "web-app/lib/supabaseServer.ts",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/middleware.ts.eta",
      "to": "web-app/middleware.ts",
      "strategy": "create-or-overwrite"
    }
  ]
}
