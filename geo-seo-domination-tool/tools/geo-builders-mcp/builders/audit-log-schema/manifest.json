{
  "id": "audit-log-schema",
  "title": "Audit Log Schema + RLS",
  "summary": "Creates audit_logs table with automatic user tracking, RLS policies, and helper functions for logging events.",
  "engine": "eta",
  "variables": [
    {
      "name": "SCHEMA_NAME",
      "required": false,
      "example": "public"
    },
    {
      "name": "TABLE_NAME",
      "required": false,
      "example": "audit_logs"
    }
  ],
  "files": [
    {
      "from": "templates/sql/audit-logs.sql.eta",
      "to": "database/supabase/audit_logs.sql",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/README.md.eta",
      "to": "docs/builders/audit-log-schema.md",
      "strategy": "create-or-overwrite"
    }
  ]
}
