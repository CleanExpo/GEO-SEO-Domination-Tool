{
  "id": "workspace-pages",
  "title": "Workspaces Pages + API",
  "summary": "Adds /workspaces UI (list/create/switch), API to create/list, and helper to read active workspace from cookie.",
  "engine": "eta",
  "variables": [
    {
      "name": "DEFAULT_SLUG",
      "required": false,
      "example": "main"
    }
  ],
  "files": [
    {
      "from": "templates/app/workspaces/page.tsx.eta",
      "to": "web-app/app/workspaces/page.tsx",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/app/api/workspaces/route.ts.eta",
      "to": "web-app/app/api/workspaces/route.ts",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/lib/workspace.ts.eta",
      "to": "web-app/lib/workspace.ts",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/README.md.eta",
      "to": "docs/builders/workspace-pages.md",
      "strategy": "create-or-overwrite"
    }
  ]
}
