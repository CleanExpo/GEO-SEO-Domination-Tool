# MCP Server Configuration
# Copy to .env and configure

# Docker Settings
COMPOSE_PROJECT_NAME=geo-seo-mcp
DOCKER_BUILDKIT=1

# Puppeteer Settings
PUPPETEER_PORT=3100
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium

# Playwright Settings
PLAYWRIGHT_PORT=3101
PLAYWRIGHT_BROWSERS=chromium,firefox,webkit

# Resource Limits
MCP_MEMORY_LIMIT=2g
MCP_CPU_LIMIT=2

# Logging
LOG_LEVEL=info
