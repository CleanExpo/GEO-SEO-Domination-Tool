{
  "id": "plan-caps",
  "title": "Plan Default Caps (free/pro/admin)",
  "summary": "Creates plan_defaults table with JSON caps, Plans settings page to edit, API to read/update, and helpers to bootstrap usage_limits per workspace.",
  "engine": "eta",
  "variables": [],
  "files": [
    {
      "from": "templates/sql/plan_defaults.sql.eta",
      "to": "database/supabase/plan_defaults.sql",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/app/settings/plans/page.tsx.eta",
      "to": "web-app/app/settings/plans/page.tsx",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/app/api/plans/route.ts.eta",
      "to": "web-app/app/api/plans/route.ts",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/lib/plans.ts.eta",
      "to": "web-app/lib/plans.ts",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/README.md.eta",
      "to": "docs/builders/plan-caps.md",
      "strategy": "create-or-overwrite"
    }
  ]
}
