{
  "id": "workspace-schema",
  "title": "Workspaces Schema + RLS",
  "summary": "Creates workspaces, memberships, and projects tables with row-level security and role-based policies.",
  "engine": "eta",
  "variables": [
    {
      "name": "SCHEMA_NAME",
      "required": false,
      "example": "public"
    },
    {
      "name": "WORKSPACES_TABLE",
      "required": false,
      "example": "workspaces"
    },
    {
      "name": "MEMBERSHIPS_TABLE",
      "required": false,
      "example": "workspace_memberships"
    },
    {
      "name": "PROJECTS_TABLE",
      "required": false,
      "example": "projects"
    }
  ],
  "files": [
    {
      "from": "templates/sql/workspaces.sql.eta",
      "to": "database/supabase/workspaces.sql",
      "strategy": "create-or-overwrite"
    },
    {
      "from": "templates/README.md.eta",
      "to": "docs/builders/workspace-schema.md",
      "strategy": "create-or-overwrite"
    }
  ]
}
